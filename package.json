{
  "name": "@redhat-cloud-services/frontend-components-inventory-patchman",
  "version": "1.8.0",
  "browser": "dist/index.js",
  "private": false,
  "dependencies": {
    "@patternfly/react-core": "^4.84.4",
    "@patternfly/react-icons": "^4.7.22",
    "@patternfly/react-table": "^4.19.45",
    "@redhat-cloud-services/frontend-components": "^2.5.8",
    "@redhat-cloud-services/frontend-components-notifications": "^2.2.4",
    "@redhat-cloud-services/frontend-components-remediations": "^2.3.8",
    "@redhat-cloud-services/frontend-components-translations": "^2.2.1",
    "@redhat-cloud-services/frontend-components-utilities": "^2.2.9",
    "axios": "^0.21.1",
    "classnames": "^2.2.6",
    "html-webpack-plugin": "^4.0.0-alpha.1",
    "query-string": "^6.13.8",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-redux": "^7.2.2",
    "react-router-dom": "^5.2.0",
    "redux": "^4.0.5",
    "redux-promise-middleware": "^6.1.2",
    "toposort": "^2.0.2"
  },
  "jest": {
    "coverageDirectory": "./coverage/",
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src/**/*.js",
      "!src/**/stories/*"
    ],
    "setupFiles": [
      "<rootDir>/config/setupTests.js"
    ],
    "roots": [
      "<rootDir>/src/"
    ],
    "moduleNameMapper": {
      "\\.(css|scss)$": "identity-obj-proxy"
    }
  },
  "devDependencies": {
    "@babel/core": "^7.12.10",
    "@babel/eslint-parser": "^7.12.1",
    "@babel/plugin-proposal-class-properties": "^7.12.1",
    "@babel/plugin-proposal-object-rest-spread": "^7.12.1",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.12.10",
    "@babel/preset-env": "^7.12.11",
    "@babel/preset-flow": "^7.12.1",
    "@babel/preset-react": "^7.12.10",
    "@babel/runtime": "^7.12.5",
    "@formatjs/cli": "^3.0.1",
    "@redhat-cloud-services/frontend-components-config": "3.0.0-beta17",
    "@rollup/plugin-commonjs": "^17.0.0",
    "@rollup/plugin-image": "^2.0.6",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^11.1.0",
    "@rollup/pluginutils": "^4.1.0",
    "@semantic-release/changelog": "^5.0.1",
    "@semantic-release/git": "^9.0.0",
    "@semantic-release/npm": "^7.0.10",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^26.6.3",
    "babel-plugin-dual-import": "^1.2.1",
    "babel-plugin-lodash": "^3.3.4",
    "babel-plugin-transform-imports": "^2.0.0",
    "babel-plugin-transform-inline-environment-variables": "^0.4.3",
    "css-loader": "^5.0.1",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.5",
    "enzyme-to-json": "^3.6.1",
    "eslint": "^7.18.0",
    "eslint-config-prettier": "^7.1.0",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-react": "^7.22.0",
    "glob": "^7.1.6",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^26.6.3",
    "node-sass": "^5.0.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.2.4",
    "prop-types": "^15.7.2",
    "react-intl": "^5.10.16",
    "redux-mock-store": "^1.5.4",
    "rollup": "^2.36.2",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-node-globals": "^1.4.0",
    "rollup-plugin-postcss": "^4.0.0",
    "rollup-plugin-terser": "^7.0.2",
    "semantic-release": "^17.3.3",
    "stylelint": "^13.8.0",
    "stylelint-config-recommended-scss": "^4.2.0",
    "stylelint-scss": "^3.18.0",
    "webpack": "^5.15.0",
    "webpack-bundle-analyzer": "^4.3.0",
    "webpack-cli": "^4.3.1"
  },
  "scripts": {
    "build": "webpack --config config/prod.webpack.config.js --mode production",
    "analyze": "NODE_ENV=production webpack --config config/prod.webpack.config.js --env analyze=true --mode production",
    "test": "jest --verbose",
    "test:update": "jest --updateSnapshot --passWithNoTests",
    "lint": "npm-run-all lint:*",
    "lint:js": "eslint config src",
    "lint:js:fix": "eslint config src --fix",
    "lint:sass": "stylelint 'src/**/*.scss' --config .stylelintrc.json",
    "prod": "NODE_ENV=production webpack serve --config config/dev.webpack.config.js",
    "server:ctr": "node src/server/generateServerKey.js",
    "start": "NODE_ENV=development webpack serve --config config/dev.webpack.config.js",
    "build:prod": "NODE_ENV=production webpack --config config/prod.webpack.config.js",
    "build:components": "npm run build:components:js && npm run build:components:esm && npm run build:components:umd",
    "build:components:js": "BABEL_ENV=cjs rollup -c ./config/rollup.config.js --format=cjs --environment NODE_ENV:production,FORMAT:cjs",
    "build:components:esm": "BABEL_ENV=esm rollup -c ./config/rollup.config.js --environment NODE_ENV:production,FORMAT:esm",
    "build:components:umd": "rollup -c ./config/rollup.config.js --format=umd --environment NODE_ENV:production,FORMAT:umd",
    "deploy": "npm-run-all build:prod lint test",
    "verify": "npm-run-all build lint test",
    "nightly": "npm run deploy",
    "semantic-release": "semantic-release",
    "semantic-release-local": "semantic-release --prepare --debug=false",
    "translations": "npm-run-all translations:*",
    "translations:extract": "npx formatjs extract ./src/*.js --out-file ./build/messages/src/Messages.json --extract-from-format-message-call",
    "translations:compile": "npx formatjs compile ./build/messages/src/Messages.json --out-file ./locales/en.json"
  },
  "release": {
    "analyzeCommits": {
      "preset": "angular",
      "releaseRules": [
        {
          "type": "refactor",
          "release": "patch"
        },
        {
          "type": "style",
          "release": "patch"
        },
        {
          "type": "update",
          "release": "patch"
        }
      ]
    },
    "getLastRelease": "last-release-git",
    "publish": [
      "@semantic-release/github",
      "@semantic-release/npm"
    ],
    "verifyConditions": [
      "@semantic-release/github"
    ],
    "fail": "",
    "prepare": [
      "@semantic-release/npm",
      "@semantic-release/changelog",
      [
        "@semantic-release/git",
        {
          "message": "chore(release): ${nextRelease.version}\n\n${nextRelease.notes}"
        }
      ]
    ]
  },
  "files": [
    "/dist"
  ],
  "insights": {
    "appname": "patch"
  }
}
