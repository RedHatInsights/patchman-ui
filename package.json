{
  "name": "patchman-ui",
  "version": "0.0.1",
  "private": false,
  "dependencies": {
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/runtime": "^7.8.4",
    "@patternfly/react-core": "^3.136.11",
    "@patternfly/react-icons": "^3.14.42",
    "@redhat-cloud-services/frontend-components": "1.0.2",
    "@redhat-cloud-services/frontend-components-notifications": "1.0.1",
    "@redhat-cloud-services/frontend-components-remediations": "1.0.0",
    "@redhat-cloud-services/frontend-components-utilities": "1.0.0",
    "axios": "^0.19.0",
    "classnames": "^2.2.5",
    "html-webpack-plugin": "^4.0.0-alpha.1",
    "react": "16.12.0",
    "react-dom": "16.12.0",
    "react-redux": "^7.1.0",
    "react-router-dom": "^4.3.1",
    "redux": "^3.7.2",
    "redux-logger": "^3.0.6",
    "redux-promise-middleware": "^5.1.1",
    "toposort": "^1.0.4"
  },
  "jest": {
    "coverageDirectory": "./coverage/",
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src/**/*.js",
      "!src/**/stories/*"
    ],
    "setupFiles": [
      "<rootDir>/config/setupTests.js"
    ],
    "roots": [
      "<rootDir>/src/"
    ],
    "moduleNameMapper": {
      "\\.(css|scss)$": "identity-obj-proxy"
    }
  },
  "devDependencies": {
    "@babel/core": "^7.8.4",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "@babel/plugin-transform-runtime": "^7.1.0",
    "@babel/preset-env": "^7.8.4",
    "@babel/preset-flow": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "@patternfly/react-table": "^2.26.16",
    "@redhat-cloud-services/frontend-components-config": "1.0.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^24.1.0",
    "babel-plugin-dual-import": "^1.2.1",
    "babel-plugin-lodash": "^3.3.2",
    "cross-fetch": "^2.2.2",
    "css-loader": "^2.1.1",
    "enzyme": "^3.6.0",
    "enzyme-adapter-react-16": "^1.5.0",
    "enzyme-to-json": "^3.3.4",
    "eslint": "^4.19.1",
    "eslint-config-prettier": "^2.10.0",
    "eslint-loader": "^2.1.0",
    "eslint-plugin-react": "^7.18.3",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^24.1.0",
    "node-sass": "^4.12.0",
    "npm-run-all": "^4.1.3",
    "postcss": "^7.0.16",
    "prop-types": "^15.6.2",
    "stylelint": "^9.10.1",
    "stylelint-config-recommended-scss": "^3.2.0",
    "stylelint-scss": "^3.3.0",
    "webpack": "^4.17.2",
    "webpack-bundle-analyzer": "^3.3.2",
    "webpack-cli": "^3.2.3"
  },
  "scripts": {
    "build": "webpack --config config/prod.webpack.config.js",
    "test": "jest --verbose",
    "lint": "npm-run-all lint:*",
    "lint:js": "eslint config src",
    "lint:js:fix": "eslint config src --fix",
    "lint:sass": "stylelint 'src/**/*.scss' --config .stylelintrc.json",
    "prod": "NODE_ENV=production webpack-dev-server --config config/dev.webpack.config.js",
    "server:ctr": "node src/server/generateServerKey.js",
    "start": "NODE_ENV=development webpack-dev-server --config config/dev.webpack.config.js",
    "build:prod": "NODE_ENV=production webpack --config config/prod.webpack.config.js",
    "deploy": "npm-run-all build:prod lint test",
    "verify": "npm-run-all build lint test",
    "nightly": "npm run deploy"
  },
  "insights": {
    "appname": "patch"
  }
}
